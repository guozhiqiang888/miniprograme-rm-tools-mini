page{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: scroll;
  font-family: "SF Pro Display";
}
.sales-waper{
  width: 100%;
  height: 100px;
  font-size: 33px;
  color: #333333;
  letter-spacing: 0.4px;
  line-height: 40px;
  position: relative;
  background-color: #EDEDED;
}
.label-waper{
  position: absolute;
  bottom: 16px;
  left: 16px;
  font-family: 'SF Pro Regular';
}
.icon_poson{
  right: 16px;
  position: absolute;
}
.icon_poson_close{
  width: 16px;
}
.icon-person{
  width: 18px;
  height: 18px;
}
.search-waper{
  /* width: 92%; */
  padding: 16px 16px 0px;
  background-color: #fff;
}
.search-box{
  width: 100%;
  height: 44px;
  display: flex;
}
.search-input{
  flex: 1;
  position: relative;
  height: 44px;
  padding-left: 30px;
  padding-right: 30px;
  font-size: 19px;
  font-weight: 300;	
  font-family: 'Hsbc Light';
  letter-spacing: -0.32px;	
  line-height: 24px;
  color: #252525;
  background-repeat: no-repeat;
  background-size: 17.7px 17.7px;
  background-position-x: 8px;
  background-position-y: center;
  /* background-color: #EDEDED; */
  background-image: url(data:image/png;base64,)
}
.search-input i{
  display: none;
  position: absolute;
  width: 17.6px;
  height: 17.6px;
  right: 9px;
  top: 50%;
  margin-top: -8.8px;
  background: url(data:image/png;base64,) no-repeat 0 0 / 100% 100%;
}
.search-input.current i{
  display: block;
}
.search-box span{
  display: none;
  font-size: 19px;
  font-family: 'Hsbc Regular';
  padding-left: 16px;
  height: 100%;
  align-items: center;
  color: #767676;
}
.search-box span.current{
  display: flex;
}
.filter-label{
  width: 100%;
  padding: 5px 0px;
  font-size: 14px;
}

.filter-waper{
  width: 100%;
  height: 40px;
  display: flex;
  font-size: 12px;
  border-bottom: 1px solid #EDEDED;
}
.items{
  display: flex;
  flex: 1;
  height: 100%;
  align-items: center;
  justify-content: center;
  border-bottom: 2px solid rgba(0, 0, 0, 0);
  color: #333333;	
  font-family: "SF Pro Medium";	
  font-size: 14px;	
  font-weight: 300;	
  letter-spacing: -0.15px;	
  box-sizing: border-box;
}
/* .item-last{
  width: 40%;
  height: 100%;
  display: flex;
  align-items: center;
} */
.item-active{
  /* background-color: #DB0011; */
  border-color: #DB0011;
  /* color: #fff; */
}
.item-last-title{
  width: 80%;
  text-align: center;
}
.filter-icon{
  width: 18px;
  height: 18px;
  margin-right: 20px;
}
.content-wapper{
  /* width: 92%; */
  padding: 16px 16px;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  border-bottom: 1px solid #EDEDED;
}
.proity{
  width: 35px;
  height: 18px;
  color: #fff;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  border-radius: 2px;
  background-color: #A8000B;
	font-family: "SF Pro Regular";	
  font-size: 12px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
}
.proity.mid{
  background-color: #Ffbb33;
}
.proity.low{
  background-color: #00847F;
}

.content{
  flex: auto;
  margin-left: 5px;
  text-align: left;
}

.icon-right{
  width: 10px;
  background-size: 10px 15px;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url(data:image/png;base64,);
  flex-shrink: 0;
}
.com-name{
  width: 100%;
  height: 18px;
  display: flex;
  align-items: center;
  margin-bottom: 5px;
  color: #333333;	
  font-family: "SF Pro Medium";	
  font-size: 16px;	
  font-weight: 500;	
  letter-spacing: -0.32px;
}
.icon{
  display: none;
  width: 5px;
  height: 5px;
  border-radius: 10px;
  margin-left: 4px;
  background-color: #A8000B;
  align-self: flex-start;
}
.icon.current{
  display: block;
}
.item-status{
  width: 100%;
  height: 20px;
  line-height: 1;
  display: flex;
  align-items: flex-end;
  padding-bottom: 4px;
}
.item-status:last-of-type{
  padding-bottom: 0;
}
.item-font{
  color: #767676;
  font-family: "SF Pro Light";	
  font-size: 14px;	
  font-weight: 300;	
  letter-spacing: -0.15px;
}
.item-date{
  margin-left: 5px;
  color: #333333;	
  font-family: "SF Pro Medium";	
  font-size: 16px;	
  font-weight: 500;	
  letter-spacing: 0.33px;
}
.icon-warning{
  display: none;
  width: 20px;
  height: 20px;
  margin-left: 5px;
  background-position: center 3px;
  background-repeat: no-repeat;
  background-size: 16px 16px;
  background-image: url(data:image/png;base64,)
}
.icon-warning.current{
  display: block;
}
.filter-page{
  width: 100%;
  height: 100%;
  background-color: #fce;
  /* position: absolute; */
  /* z-index: 1; */
}


.mCostDetail{
  position: fixed;
  left: 0;
  top: 100%;
  width: 100%;
  height: 100%;
  z-index: 9999;
}
.mCostDetail__bg{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}
.mCostDetail__box{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  z-index: 100;
}

/* filter page */

.filter-content-waper{
  width: 93%;
  height: 94%;
  padding: 16px 16px;
}
.closebar{
  width: 100%;
  height: 20px;
  display: flex;
  font-size: 18px;
  justify-content: space-between;
  align-items: center;
}
.icon-close{
  width: 15px;
  height: 15px;
}
.headlist-waper{
  width: 100%;
  margin-top: 20px;
}
.header{
  width: 100%;
  color: #333333;
  padding: 10px 0;
  font-family: "SF Pro Text";	
  font-size: 14px;	
  font-weight: 300;	
  letter-spacing: -0.15px;	
  line-height: 20px;
}
.list-item{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px 0px;
  font-weight: 400;
  border-bottom: 1px solid #EDEDED;
  color: #333333;	
  font-family: "SF Pro Text";	
  font-size: 16px;	
  letter-spacing: -0.32px;	
  line-height: 24px;
}
radio .wx-radio-input {
  display: none;
}
.radio-group{
  width: 100%;
}
.radio-new{
  border: 1px solid #767676;
  position: absolute;
  padding: 2px;
  right: 16px;
  width: 14px;
  height: 14px;
  border-radius: 14px 14px;
}
.radio-new[checked]{
  background-color: #38837f;
  background-clip: content-box;
}
.radio-new-bulk{
  border: 1px solid #767676;
  position: absolute;
  padding: 2px;
  right: 16px;
  width: 14px;
  height: 14px;
  border-radius: 14px 14px;
}
checkbox .wx-checkbox-input{
  display: none;
}
.radio-new-bulk[checked]{
  border: none;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 25px 25px;
  background-image: url(data:image/png;base64,)
}
.radio-margin{
  padding: 15px 0px;
  width: 100%; 
  border-bottom: 1px solid #EDEDED;
  color: #333333;	
  font-family: "SF Pro Text";	
  font-size: 16px;	
  letter-spacing: -0.32px;	
  line-height: 24px;
}
.img-icon-right{
  width: 10px;
  height: 15px;
}
.warning-waper{
  width: 93%;
  position: fixed;
  top: 0px;
  padding: 16px 16px 0px;
  color: #fff;
  background-color: #01847F;
}
.waring-header{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.war-head-right{
  display: flex;
  align-items: center;
}
.img-icon{
  width: 20px;
  height: 20px;
  margin-right: 10px;
}
.down{
  width: 100%;
  display: flex;
  padding: 16px 0px;
  font-size: 14px;
  flex-direction: row-reverse;
}
.down1{
  margin-left: 20px;
}





.content-alert{
    width: 93%;
    height: 100%;
    top: 0px;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    background-color: #ccc;
    padding: 0px 16px;
    background: rgba(0,0,0,0.5);
    overflow: hidden;
    z-index: 100000;
}
 .alert-content{
    width: 100%;
    display: flex;
    flex-flow: column;
    align-items: center;
    padding: 0px 16px;
    background-color: white;
}
.title{
    width: 100%;
    padding: 15px 0px;
    font-size: 20px;
}
.content-msg{
    width: 100%;
    font-size: 16px;
}
 .btns{
    width: 100%;
    display: flex;
    text-align: center;
    justify-content: space-between;
    padding: 20px 0px 15px 0px;
}
.btns1{
    width: 47%;
    height: 45px;
    border: 1px solid #000;
}
.disable{
    background-color: #fff;
}
.enable{
    color: white;
    border: none;
    background-color: #CA2A23;
} 