/* pages/rmhome/rmhome.wxss */
page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  font-family: "SF Pro Display";
}
.sales-waper{
  width: 100%;
  height: 100px;
  font-size: 33px;
  color: #333333;
  letter-spacing: 0.4px;
  line-height: 40px;
  position: relative;
  background-color: #EDEDED;
}
.label-waper{
  position: absolute;
  bottom: 16px;
  left: 16px;
}
.icon_poson{
  width: 20px;
  height: 20px;
  right: 20px;
  top:20px;
  position: absolute;
  background-color: #cef;
}
.search-waper{
  width: 93%;
  padding: 16px 16px 0px;
  background-color: #fff;
}
.search-box{
  width: 100%;
  height: 44px;
  background-color: #EDEDED;
}
.search-input{
  height: 44px;
  padding-left: 30px;
  padding-right: 30px;
  font-size: 16px;
  font-weight: 300;	
  letter-spacing: -0.32px;	
  line-height: 24px;
  color: #333333;
  background-repeat: no-repeat;
  background-size: 16px 16px;
  background-position-x: 8px;
  background-position-y: 14px;
  background-image: url(data:image/png;base64,)
}
.filter-label{
  width: 100%;
  padding: 5px 0px;
  font-size: 14px;
}

.filter-waper{
  width: 100%;
  height: 40px;
  display: flex;
  line-height: 40px;
  font-size: 12px;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #EDEDED;
}
.items{
  width: 30%;
  height: 90%;
  text-align: center;
  justify-content: center;
  border-right:1px solid #C9C9C9;
}
.item-last{
  width: 40%;
  height: 100%;
  display: flex;
  align-items: center;
}
.item-active{
  background-color: #DB0011;
  color: #fff;
}
.item-last-title{
  width: 80%;
  text-align: center;
}
.filter-icon{
  width: 20px;
  height: 20px;
  right: 16px;
  position: absolute;
}
.content-wapper{
  width: 93%;
  padding: 16px 16px;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  border-bottom: 1px solid #EDEDED;
}
.proity{
  width: 35px;
  height: 18px;
  color: #fff;
  white-space: normal;
  text-overflow: ellipsis;
  border-radius: 2px;	
  font-size: 12px;	
  line-height: 16px;	
  text-align: center;
  background-color: #A8000B;
}

.content{
  width: 90%;
  margin-left: 5px;
  text-align: left;
}

.icon-right{
  width: 10px;
  background-size: 10px 15px;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url(data:image/png;base64,)
}
.com-name{
  width: 100%;
  display: flex;
  font-weight: bold;
  padding-bottom: 5px;
}
.icon{
  width: 5px;
  height: 5px;
  border-radius: 10px;
  margin-left: 4px;
  background-color: #A8000B;
}
.item-status{
  width: 100%;
  display: flex;
  padding-bottom: 1px;
}
.item-font{
  line-height: 24px;
  font-size: 14px;
  color: #333333;
}
.item-date{
  font-size: 16px;
  font-weight: 500;
  margin-left: 5px;
}
.icon-warning{
  width: 20px;
  height: 20px;
  margin-left: 5px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 16px 16px;
  background-image: url(data:image/png;base64,)
}