page{
  display: flex;
  align-items: center;
  padding: 0 20px;
  box-sizing: border-box;
}
icon{
  display: block;
  width: 48px;
  margin: 0 auto;
}
header{
  display: block;
  text-align: center;
  font-size: 28px;
  margin-top: 16px;
  color: #333;
}
text{
  margin-top: 10px;
  display: block;
  text-align: center;
  font-size: 16px;
  line-height: 24px;
  color: #333;
}
navigator{
  display: flex;
  height: 48px;
  justify-content: center;
  align-items: center;
  font-size: 16px;
  color: #333;
  border: 1px solid #262626;
  margin: 30px 0 15vh 0;
}