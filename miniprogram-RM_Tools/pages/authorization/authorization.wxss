page{
  display: flex;
  align-items: center;
  padding: 0 20px;
  box-sizing: border-box;
}
icon{
  display: block;
  width: 93px;
  margin: 0 auto;
}
header{
  display: block;
  text-align: center;
  font-size: 20px;
  margin-top: 25px;
}
text{
  margin-top: 20px;
  display: block;
  text-align: center;
  font-size: 14px;
  line-height: 21px;
  color: #888;
}
button,navigator{
  margin-top: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 47px;
  line-height: 47px;
  font-size: 18px;
  color: #fff;
  border: 1px solid #1AAD19;
  background: #1AAD19;
  border-radius: 5px;
}
button::after{
  border: none;
}
navigator{
  margin: 10px 0 15vh 0;
  background: #f8f8f9;
  border-color:rgba(5, 5, 5, 0.1);
  color: #000;
}